{
    "name": "seminar-ts-server",
    "version": "1.0.0",
    "description": "Server for hosting seminar rooms written in Typescript",
    "main": "dist/main.js",
    "scripts": {
        "format": "prettier --write .",
        "format-check": "prettier --check .",
        "build": "shx rm -rf dist/ && tsc",
        "build:watch": "tsc --watch",
        "start": "node dist/main.js",
        "dev": "nodemon -x ts-node src/main.ts"
    },
    "author": "Benjamin W. Portner",
    "license": "MIT",
    "dependencies": {
        "@socket.io/admin-ui": "^0.5.1",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^5.1.0",
        "moment": "^2.30.1",
        "socket.io": "^4.8.1",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@eslint/css": "^0.6.0",
        "@eslint/js": "^9.24.0",
        "@eslint/json": "^0.11.0",
        "@eslint/markdown": "^6.3.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^5.0.1",
        "@types/moment": "^2.11.29",
        "@types/socket.io": "^3.0.1",
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "shx": "^0.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.29.1"
    },
    "engines": {
        "node": ">=22.0.0"
    }
}
